                "label"             : "Phase-1",
                "legendEntry"       : "Phase-1",
                "folder"            : "",
                "plot"             : "Ph1hT0_st1"
           },
            "Phase-2"   :
            {
                "label"             : "Phase-2",
                "legendEntry"       : "Phase-2",
                "folder"            : "",
                "plot"             : "Ph2hT0_st1"
           }
       },
        "plot"      :
